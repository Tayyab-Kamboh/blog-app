<h1>All Users</h1>
<p><%= @placeholder_text %></p>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user_path(user) do %>
        <%= user.name %>
        <% if user.photo.present? %>
          <img src="<%= user.photo %>" alt="User Photo" width="100px">
        <% else %>
          <p>No photo available</p>
        <% end %>
      <% end %>
    </li>
  <% end %>
</ul>
